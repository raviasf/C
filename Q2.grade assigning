/*2. Assign grades based on marks:
Marks ≥ 90 → A, Marks ≥ 75 → B, Marks ≥ 50 → C, Else → F.*/

//Level 01

#include <stdio.h>
int main()
{
  float grade;
  printf("Enter marks obtained:\n");
  scanf("%f",&grade);
  if(grade<=50) printf("Fail");
  else if(grade<=75) printf("Grade-C");
  else if(grade<=90) printf("Grade-B");
  else printf("Grade-A");
return 0;
}
