//8. Write a program to classify a character as vowel or consonant using switch case.

//Level01

#include <stdio.h>
int main()
{
  char input;
  printf("Enter any letter:\n");
  scanf("%c",&input);
  switch(input)
  {
    case 'a':
    case 'A':
    case 'e':
    case 'E':
    case 'i':
    case 'I':
    case 'o':
    case 'O':
    case 'u':
    case 'U':
      printf("Its a vowel.");
    break;
    default:
      printf("Its a consonant.");
  }
return 0;
}
